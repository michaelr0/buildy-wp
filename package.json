{
  "name": "handmadeweb-webpack",
  "version": "1.0.0",
  "description": "A WordPress starter theme with webpack & vue.js.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/_s.pot' --domain 'hmw'"
  },
  "homepage": "https://github.com/carrieforde/_s-with-webpack#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11"
  ],
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-optional-chaining": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "autoprefixer": "^9.6.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^5.0.2",
    "css-mqpacker": "^7.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "img-loader": "^3.0.2",
    "mini-css-extract-plugin": "^1.3.8",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "svg-sprite-loader": "^5.2.1",
    "svgo": "^2.1.0",
    "svgo-loader": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "wp-pot-cli": "^1.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "baguettebox.js": "^1.11.1",
    "extract-svg-sprite-webpack-plugin": "^0.8.1",
    "macy": "^2.5.1",
    "siema": "^1.5.1",
    "yarn": "^1.22.10"
  }
}